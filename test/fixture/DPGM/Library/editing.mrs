'Removes DK and/or Other if coded as well as another code
Sub sbCleanDKandOther(qst,code1,code2)
Select case (qst.AnswerCount())
    Case 2
        If Not (qst.ContainsAny(CCategorical(code1)) and qst.containsany(CCategorical(code2))) Then
            If qst.ContainsAny(CCategorical(code1)) then qst = qst - CCategorical(code1)
            If qst.ContainsAny(CCategorical(code2)) then qst = qst - CCategorical(code2)
        Else
            If qst.ContainsAny(CCategorical(code1)) then qst = qst - CCategorical(code1)
        End if
    Case >=3
        If qst.ContainsAny(CCategorical(code1)) then qst = qst - CCategorical(code1)
        If qst.ContainsAny(CCategorical(code2)) then qst = qst - CCategorical(code2)
End Select
End Sub

'Removes DK if another code
Sub sbCleanRemoveCombinedDK (qst,code1)
Select case (qst.AnswerCount())
    Case >=2
        If qst.ContainsAny(CCategorical(code1)) Then qst = qst - CCategorical(code1)
End Select
End Sub
