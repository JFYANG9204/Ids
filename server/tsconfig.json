{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": ["ES2019"],
    "strict": true,
    "outDir": "./out",
    "rootDir": "./src",
    "composite": true,
    "suppressImplicitAnyIndexErrors": true,
    "downlevelIteration": true
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
