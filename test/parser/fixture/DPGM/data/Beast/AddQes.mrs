sub sbAddBeastQesList(objMDM,var,desc,list,fso)


'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        dim file,file1,file_g,file_t,ele,cnt,checker

        if fso.FileExists("output\temp.inc") then
                set file=fso.OpenTextFile("output\temp.inc",8) 'ForAppending
        else
                set file=fso.createtextfile("output\temp.inc",true,false)
        end if

        if fso.FileExists("output\Txt_DelUseless.txt") then
                set file1=fso.OpenTextFile("output\Txt_DelUseless.txt",8) 'ForAppending
        else
                set file1=fso.createtextfile("output\Txt_DelUseless.txt",true,false)
'                file1.WriteLine("    SelectQuery = ""select "" + RESP_NUMBER + "" _")
                file1.WriteLine("    SelectQuery = ""select Respondent_Serial_")
        end if

        if fso.FileExists("output\CreatGrid.mrs") then
                set file_g=fso.OpenTextFile("output\CreatGrid.mrs",8) 'ForAppending
        else
                set file_g=fso.createtextfile("output\CreatGrid.mrs",true,false)
        end if

        if fso.FileExists("output\tab.mrs") then
                set file_t=fso.OpenTextFile("output\tab.mrs",8) 'ForAppending
        else
                set file_t=fso.createtextfile("output\tab.mrs",true,false)
        end if

'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

        with objMDM


             file.WriteLine(var+" """+desc+""" categorical[1]")
             file.WriteLine("{")
             file.WriteLine("  use " + list)
             file.WriteLine("};")

        end with

          file1.WriteLine(","+var+"_")

          file_t.WriteLine("	fnAddTable(TableDoc,"""+var+"{e1 '' text(),base 'Base : Total Respondent' base(),e2 '' text(),..,e3 '' text(),sigma 'Sigma' subtotal()}"",""axis({b1 '' base()[ishidden=true], bk1 'Total' expression('""+ GLOBALFILTER +""')})"",Null,"""")")
          file_t.Close()
end sub

sub sbAddBeastQesSC11a(objMDM,var,desc,desc_1,desc_2,fso,bn[])

'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

        dim file,file1,file_g,file_t,ele,cnt,checker, i, axisname

        if fso.FileExists("output\temp.inc") then
                set file=fso.OpenTextFile("output\temp.inc",8) 'ForAppending
        else
                set file=fso.createtextfile("output\temp.inc",true,false)
        end if

        if fso.FileExists("output\Txt_DelUseless.txt") then
                set file1=fso.OpenTextFile("output\Txt_DelUseless.txt",8) 'ForAppending
        else
                set file1=fso.createtextfile("output\Txt_DelUseless.txt",true,false)
                file1.WriteLine("    SelectQuery = ""select Respondent_Serial_")
        end if

        if fso.FileExists("output\CreatGrid.mrs") then
                set file_g=fso.OpenTextFile("output\CreatGrid.mrs",8) 'ForAppending
        else
                set file_g=fso.createtextfile("output\CreatGrid.mrs",true,false)
        end if

        if fso.FileExists("output\tab.mrs") then
                set file_t=fso.OpenTextFile("output\tab.mrs",8) 'ForAppending
        else
                set file_t=fso.createtextfile("output\tab.mrs",true,false)
        end if

'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

				For i = 1 to ubound(bn)

				if i < 10 then axisname = "0" + cText(i)
				if i >= 10 then axisname = cText(i)

        with objMDM


             file.WriteLine(var+"_"+axisname+" """+desc+" - "+bn[i]+""" categorical[1]")
             file.WriteLine("{")
             file.WriteLine("N5 ""-5 - "+desc_1+""" factor(1),")
             file.WriteLine("N4 ""-4"" factor(2),")
             file.WriteLine("N3 ""-3"" factor(3),")
             file.WriteLine("N2 ""-2"" factor(4),")
             file.WriteLine("N1 ""-1"" factor(5),")
             file.WriteLine("_0 ""0""  factor(6),")
             file.WriteLine("P1 ""+1"" factor(7),")
             file.WriteLine("P2 ""+2"" factor(8),")
             file.WriteLine("P3 ""+3"" factor(9),")
             file.WriteLine("P4 ""+4"" factor(10),")
             file.WriteLine("P5 ""+5 - "+desc_2+""" factor(11)")
             file.WriteLine("};")

        end with

          file1.WriteLine(","+var+"_"+axisname+"_")

          dim tmptxt

          if i=1 then tmptxt=var+"_"+axisname
          if i>1 then tmptxt=tmptxt+","+var+"_"+axisname

        Next

          file_g.WriteLine("TableDoc.DataSet.Variables.AddNewGrid("""+tmptxt+""","""+var+""","""","""+var+". "+desc+""")")
          file_t.WriteLine("	fnAddGrid(TableDoc,"""+var+"[..].Column{e1 '' text(),base 'Base : Total Respondent' base(),e2 '' text(),..,e3 '' text(),sigma 'Sigma' subtotal()}"","""+var+""",NULL,"""")")
          file_g.Close()
          file_t.Close()

end sub

